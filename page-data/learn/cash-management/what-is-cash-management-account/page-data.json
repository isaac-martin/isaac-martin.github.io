vironment":{"sys":{"id":"master","type":"Link","linkType":"Environment"}},"revision":4,"contentType":{"sys":{"type":"Link","linkType":"ContentType","id":"articleHeader"}},"locale":"en-US"},"fields":{"category":"cash-management","thumbnailImage":{"sys":{"space":{"sys":{"type":"Link","linkType":"Space","id":"lrdkz6qgi9tt"}},"id":"5I3v5ovT76afWx6HpLSJwD","type":"Asset","createdAt":"2019-10-02T16:50:49.062Z","updatedAt":"2019-10-02T16:50:49.062Z","environment":{"sys":{"id":"master","type":"Link","linkType":"Environment"}},"revision":1,"locale":"en-US"},"fields":{"title":"Thumbnail-what-is-wire-transfer","file":{"url":"//images.ctfassets.net/lrdkz6qgi9tt/5I3v5ovT76afWx6HpLSJwD/39c9024005fb15de47f550eecba1915c/Thumbnail-what-is-wire-transfer.png","details":{"size":18232,"image":{"width":273,"height":273}},"fileName":"Thumbnail-what-is-wire-transfer.png","contentType":"image/png"}}},"urlPath":"what-is-wire-transfer","title":"What is a wire transfer?"}}]}}],"urlPath":"what-is-cash-management-account","category":"cash-management","thumbnailImage":{"sys":{"space":{"sys":{"type":"Link","linkType":"Space","id":"lrdkz6qgi9tt"}},"id":"7pItUKL0tqcrAosWjBOz8P","type":"Asset","createdAt":"2019-11-18T20:50:33.226Z","updatedAt":"2019-11-18T20:50:33.226Z","environment":{"sys":{"id":"master","type":"Link","linkType":"Environment"}},"revision":1,"locale":"en-US"},"fields":{"title":"Thumbnail-cash-management-account","file":{"url":"//images.ctfassets.net/lrdkz6qgi9tt/7pItUKL0tqcrAosWjBOz8P/c06a73d3189339b7e94a80bcd0078915/Thumbnail-cash-management-account.png","details":{"size":20044,"image":{"width":273,"height":273}},"fileName":"Thumbnail-cash-management-account.png","contentType":"image/png"}}}}}}}